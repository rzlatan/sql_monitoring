@model IEnumerable<SQLMonitoring.Model.User>

@{
    ViewData["Title"] = "Home";
}

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>|
                @Html.ActionLink("Delete", "Delete", "User", new { id = item.Id }, new { onclick = "return confirm('Click yes to confirm deletion');" })
            </td>
        </tr>
}
    </tbody>
</table>
