                type: "stackedColumn",
                showInLegend: true,
                color: selectedColors[i],
                name: waits[i],
                dataPoints: dict[waits[i]]
            }

            dataList.push(dataObject);
        }

        var chart = new CanvasJS.Chart("wait_stats", {
            animationEnabled: true,
            title: {
                text: "Wait Stats",
                fontColor: "#0E0E0E"
            },
            axisX: {
                interval: 1,
                intervalType: "hour"
            },
            axisY: {
                valueFormatString: "#0",
                gridColor: "#B6B1A8",
                tickColor: "#B6B1A8"
            },
            toolTip: {
                shared: true,
                content: toolTipContent
            },
            data: dataList
        });
        chart.render();
    }

